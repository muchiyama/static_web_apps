{
    "routes": [
      {
        "route": "/login",
        "serve": "/.auth/login/google",
        "statusCode": 200
      },
      {
        "route": "/logout",
        "serve": "/.auth/logout"
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "allowedRoles": ["authenticated", "admin"],
        "statusCode": 200
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthenticated",
        "statusCode": "302",
        "serve": "/login"
      },
      {
        "errorType": "NotFound",
        "statusCode": "302",
        "serve": "/Index.html"
      }
    ]
  }