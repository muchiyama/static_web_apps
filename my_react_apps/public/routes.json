{
    "routes": [
      {
        "route": "/login",
        "serve": "/.auth/login/google",
        "statusCode": 200
      },
      {
        "route": "/index/*",
        "serve": "/Index.html",
        "allowedRoles": ["admin"],
        "statusCode": 200
      }
    ],
    "platformErrorOverrides": [
      {
        "errorType": "Unauthenticated",
        "statusCode": "302",
        "serve": "/login"
      }
    ]
  }